{
  "baseURL": "https://oz.blackmad.com",
  "languageCode": "en-us",
  "title": "Oz/Blackman Wedding",
  "show_comments": false,
  "disableKinds": [
    "sitemap",
    "RSS"
  ],
  "markup": {
    "defaultMarkdownHandler": "goldmark",
    "goldmark": {
      "extensions": {
        "definitionList": true,
        "footnote": true,
        "linkify": true,
        "linkifyProtocol": "https",
        "strikethrough": true,
        "table": true,
        "taskList": true,
        "typographer": true
      },
      "parser": {
        "attribute": {
          "block": false,
          "title": true
        },
        "autoHeadingID": true,
        "autoHeadingIDType": "github"
      },
      "renderer": {
        "hardWraps": false,
        "unsafe": true,
        "xhtml": false
      }
    }
  },
  "params": {
    "background_image_1": "image/first.JPG",
    "background_image_2": "image/second.JPG",
    "background_image_3": "image/third.JPG",
    "background_image_4": "image/fourth.JPG",
    "background_image_5": "image/fifth.JPG"
  }
}